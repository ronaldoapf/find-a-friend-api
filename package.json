    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
